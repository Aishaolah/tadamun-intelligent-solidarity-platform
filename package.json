{
  "name": "tadamun-root",
  "version": "1.0.0",
  "private": true,
  "description": "Root package.json that forwards scripts to the frontend folder",
  "scripts": {
    "dev": "npm --prefix frontend run dev",
    "build": "npm --prefix frontend run build",
    "start": "npm --prefix frontend run start",
    "install:all": "npm --prefix frontend install"
  }
}
